group_by(OSI) %>% 
  summarize(median = median(weight))
  
osiweight_quant <- weight_osi1 %>% 
group_by(OSI) %>%  summarise(weight = quantile(weight, c(0.25, 0.5, 0.75)))  
            
wilcox.test(weight_osi1$weight ~ weight_osi1$OSI)

