                         ifelse(osi2.2_prepost_5$OSI_diff == 0, 'ISQ',
                         ifelse(osi2.2_prepost_5$OSI_diff > 0 & osi2.2_prepost_5$OSI_diff < 0.3, 'Worse', 'AE')))
str(osi2.2_prepost_5)    

