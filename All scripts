######################################

postOSI_1.2_60 <- subset(postOSI_1.2_1, label == "L")
str(postOSI_1.2_60)

###remove rows if by5 time not 00:60:00

postOSI_1.2_60a <- postOSI_1.2_60[postOSI_1.2_60$by5 == "1970-01-01 00:55:00",]

str(postOSI_1.2_60a)

