# combine 2 same suction variables

treatment_data <- treatment_data %>% 
mutate(suctiontype = coalesce(treatment_data$RETIRED.R.AIRWAY.SUCTION.TYPE.OLD, treatment_data$R.GOSH.AIRWAY.SUCTION.TYPE))

