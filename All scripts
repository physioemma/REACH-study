str(physio_times1)
#time catogories

physio_times2 <- physio_times1 %>% 
  mutate(time = case_when(
        hour %in% 9:16   ~"day",
      hour %in% 17:23 ~ "evening",
      hour %in% 00:8 ~ "night"))
    
  physio_times2$time = factor(
      physio_times2$time,
      level = c("day", "evening","night"))
  
t.timecatogory <- table(physio_times2$time)
print(t.timecatogory)

