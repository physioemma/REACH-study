mbp_AE_1.1d[is.na(mbp_AE_1.1d)] = 'unknown'

###create AE variable

mbp_AE_1.1d$mbp_AE <- ifelse(mbp_AE_1.1d$mbp_threshold5 == "yes" | mbp_AE_1.1d$mbp_threshold15 == "yes" | mbp_AE_1.1d$mbp_threshold30 == "yes" | mbp_AE_1.1d$mbp_threshold60 == "yes", 'Yes', 'No')

