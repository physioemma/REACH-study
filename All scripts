      unknown = ifelse(physio_data1$suctiontype == 'Nasal' | physio_data1$suctiontype == 'Nasal Prong' | physio_data1$suctiontype == 'Trache'| physio_data1$suctiontype == 'NA', "Yes","No"))
        
