      suctionclosed = ifelse(physio_data1$suctiontype == 'Endotracheal Tube - Closed' | physio_data1$suctiontype == 'Endotracheal Tube - Closed;Oral' | physio_data1$suctiontype == 'Endotracheal Tube - Closed;Oral;Nasal', "Yes", "No"),
